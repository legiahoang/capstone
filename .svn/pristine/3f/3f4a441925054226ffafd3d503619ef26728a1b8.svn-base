* {
	font-family: 'Open Sans', sans-serif;
}

#page {
    background: #f5f5f5;
}

.slidershow * {
	margin: 0;
	padding: 0;
	width: 100%;
    height: 100%;
}

.slidershow body {
	background-color: #000;
}

.
slidershow .trs {
	-webkit-transition: all ease-out .5s;
	transition: all ease-out .5s;
}

.slidershow #slider {
	position: relative;
	z-index: 1;
}

.slidershow #slider a {
	position: absolute;
	top: 0;
	left: 0;
	opacity: 0;
}

.slidershow .ativo {
	opacity: 1 !important;
}

.slidershow span {
	background-color: rgba(0, 0, 0, 0.18);
	cursor: pointer;
	border-radius: 50%;
	opacity: 1;
	position: absolute;
	bottom: 40%;
	width: 43px;
	height: 43px;
	z-index: 5;
}

.slidershow .next {
	right: 10px;
}

.slidershow .next:before, .next:after {
	left: 21px;
}

.slidershow .next:before {
	-webkit-transform: rotate(-42deg);
	transform: rotate(-42deg);
	top: 5px;
}

.slidershow .next:after {
	-webkit-transform: rotate(-132deg);
	transform: rotate(-132deg);
	top: 19px;
}

.slidershow .next:before, .next:after, .prev:before, .prev:after {
	content: "";
	height: 20px;
	background: #fff;
	width: 1px;
	position: absolute;
}

.slidershow .prev {
	left: 10px;
}

.prev:before, .prev:after {
	left: 18px;
}

.slidershow .prev:before {
	-webkit-transform: rotate(42deg);
	transform: rotate(42deg);
	top: 5px;
}

.slidershow .prev:after {
	-webkit-transform: rotate(132deg);
	top: 19px;
}

.slidershow figure:hover span {
	opacity: .76;
}

.slidershow figure {
	height: 450px;
	position: relative;
	overflow: hidden;
}

.slidershow figcaption {
	padding-left: 20px;
	color: #fff;
	font-family: "Kaushan Script", "Lato", "arial";
	font-size: 20px;
	font-weight: bold;
	width: 100%;
	position: absolute;
	bottom: 0;
	left: 0;
	line-height: 55px;
	height: 55px;
	z-index: 5;
	background: -moz-linear-gradient(top, rgba(0,0,0,0) 0%, rgba(0,0,0,0.65) 68%, rgba(0,0,0,0.65) 100%, rgba(0,0,0,0.65) 101%);
    background: -webkit-linear-gradient(top, rgba(0,0,0,0) 0%,rgba(0,0,0,0.65) 68%,rgba(0,0,0,0.65) 100%,rgba(0,0,0,0.65) 101%);
    background: linear-gradient(to bottom, rgba(0,0,0,0) 0%,rgba(0,0,0,0.65) 68%,rgba(0,0,0,0.65) 100%,rgba(0,0,0,0.65) 101%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00000000', endColorstr='#a6000000',GradientType=0 );
}

/* ============================ home ================================== */
#home_table {
	margin-bottom: 200px;
}

#home_table table {
	margin: 0px auto;
}

#home_table>table>tbody>tr:nth-child(1)>td {
	text-align: center;
	padding: 20px;
	width: 350px;
	display: table-cell;
}

#home_table>table>tbody>tr:nth-child(1)>td img {
	border-top: 1px solid #dfdfdf;
	border-right: 1px solid #dfdfdf;
	width: 100%;
	height: 250px;
	float: left;
	vertical-align: top;
}

#home_table>table>tbody>tr:nth-child(1)>td .contentImage1 {
	z-index: 1;
	position: relative;
	margin-top: 40%;
	margin-left: 20px;
	height: 0px;
	width: 0px;
	border-bottom: 50px solid #fff;
	border-left: 387px solid transparent;
}

#home_table>table>tbody>tr:nth-child(1)>td .contentImage2 {
	z-index: 2;
	height: 210px;
	position: absolute;
	margin-left: 20px;
	background: #fff;
	width: 385.5px;
	border-bottom: 2px solid #CACACA;
	border-left: 1px solid #CACACA;
}

#home_table>table>tbody>tr:nth-child(1)>td .contentImage3 {
	margin: 10px 20px 0px 30px;
}

#home_table>table>tbody>tr:nth-child(1)>td .contentImage2 .title {
	text-transform: uppercase;
	font-size: 14px;
	font-weight: bold;
}

#home_table>table>tbody>tr:nth-child(1)>td .contentImage2 .text {
    text-align: justify;
    font-size: 14px;
    line-height: 24px;
}

#home_table>table>tbody>tr:nth-child(1)>td .contentImage4 {
	width: 322.5px;
	z-index: 3;
	position: absolute;
	margin: 170px 0px 0px 20px;
}

#home_table>table>tbody>tr:nth-child(1)>td .button {
	color: #0390df;
	font-weight: bold;
	display: inline-block;
	text-decoration: none;
	border: 0;
	outline: 0;
	margin: 0 1px 10px 0;
	padding: 10px 30px;
	background: solid transparent;
	position: relative;
	right: 0px;
	bottom: 0px;
}

#home_table>table>tbody>tr:nth-child(1)>td .button:before, .button:after
	{
	border-right: 2px solid;
	content: '';
	display: block;
	height: 8px;
	margin-top: -6px;
	position: absolute;
	transform: rotate(135deg);
	right: 10px;
	top: 50%;
	width: 0;
}

#home_table>table>tbody>tr:nth-child(1)>td .button:after {
	margin-top: -1px;
	-moz-transform: rotate(45deg);
	-o-transform: rotate(45deg);
	-webkit-transform: rotate(45deg);
	transform: rotate(45deg);
}

