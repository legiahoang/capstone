    content: "";
    display: table;
}

#content .summary .right_summary {
    float: right;
    width: 43%;
}

#content .summary .left_summary {
    float: left;
    width: 55%;
    margin-top: 30px;
    text-align: -webkit-center;
}

#table_billoflading_summary > tbody > tr:nth-child(1) > td {
	text-align: center;
    background: #00A0E0;
    color: #fff;
    padding: 15px !important;
}


/* ========================== END: summary bill of lading ========================== */

/* ========================== ListReportAdmin ========================== */

.report_form .report_form_header .ui-panel-titlebar.ui-widget-header  {
    background: #00A0E0;
    color: #fff;
}

/* ========================== END: ListReportAdmin ========================== */

/* ========================== List user (admin) ========================== */

.list_user .ui-panel-titlebar.ui-widget-header {
	background: #00A0E0;
    color: #fff;
}

.list_user .ui-panel-content.ui-widget-content input {
	border: 1px solid #ccc;
	box-shadow: none;
}

.list_user .ui-selectonemenu.ui-widget .ui-selectonemenu-label {
	box-shadow: none;
}

/* ========================== END: List user (admin) ========================== */

